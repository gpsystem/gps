{
  "compilerOptions": {
    "target": "es2015",
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "@utils/*": ["./utils/*"],
      "@message/*": ["./message/*"]
    }
  },
  "include": ["**/*"],
  "exclude": ["test/**/*"]
}
