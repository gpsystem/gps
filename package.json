{
  "name": "gps",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "ts-node scripts/build",
    "check-format": "prettier . --check --ignore-path .gitignore",
    "check-spelling": "cspell --config=.cspell.json \"**/*.{md,mdx,ts,js,tsx,jsx}\"",
    "lint": "eslint . --ignore-path .gitignore",
    "prepare": "husky install",
    "prettier": "prettier . --write --ignore-path .gitignore",
    "release": "standard-version",
    "release:prerelease": "standard-version --prerelease alpha",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "test": "npm run build && jest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gpsystem/gps.git"
  },
  "keywords": [],
  "contributors": [
    "Nicolas Rodriguez (https://github.com/nicolas377)"
  ],
  "author": "GeoFS Plugin System",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gpsystem/gps/issues"
  },
  "homepage": "https://github.com/gpsystem/gps#readme",
  "dependencies": {
    "browser-extension-url-match": "~0.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "~16.2.4",
    "@commitlint/config-conventional": "~16.2.4",
    "@geps/geofs-types": "~1.0.1",
    "@types/chrome": "~0.0.188",
    "@types/expect-puppeteer": "~4.4.7",
    "@types/fs-extra": "~9.0.13",
    "@types/glob": "~7.2.0",
    "@types/html-minifier": "~4.0.2",
    "@types/jest": "~27.5.1",
    "@types/jest-environment-puppeteer": "~5.0.2",
    "@types/puppeteer": "~5.4.6",
    "@types/react": "~18.0.9",
    "@types/react-dom": "~18.0.4",
    "@types/tailwindcss": "~3.0.10",
    "@types/yargs": "~17.0.10",
    "@typescript-eslint/eslint-plugin": "~5.25.0",
    "autoprefixer": "~10.4.7",
    "babel-loader": "~8.2.5",
    "cheerio": "~1.0.0-rc.11",
    "chokidar": "~3.5.3",
    "cspell": "~5.20.0",
    "cssnano": "~5.1.9",
    "eslint": "~8.16.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-import-resolver-alias": "~1.1.2",
    "eslint-import-resolver-typescript": "~2.7.1",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "~26.2.2",
    "eslint-plugin-react": "~7.30.0",
    "fs-extra": "~10.1.0",
    "glob": "~8.0.3",
    "html-minifier": "~4.0.0",
    "husky": "~7.0.4",
    "jest": "~28.1.0",
    "jest-puppeteer": "~6.1.0",
    "postcss": "~8.4.14",
    "prettier": "~2.6.2",
    "prettier-plugin-tailwindcss": "~0.1.11",
    "puppeteer": "~13.7.0",
    "react": "~18.1.0",
    "react-dom": "~18.1.0",
    "standard-version": "~9.5.0",
    "tailwindcss": "~3.0.24",
    "ts-jest": "~28.0.2",
    "ts-loader": "~9.3.0",
    "ts-node": "~10.7.0",
    "typescript": "~4.6.4",
    "webpack": "~5.72.1",
    "yargs": "~17.5.1"
