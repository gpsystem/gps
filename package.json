{
  "name": "gps",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "ts-node scripts/build",
    "check-format": "prettier . --check --ignore-path .gitignore",
    "check-spelling": "cspell --config=.cspell.json \"**/*.{md,mdx,ts,js,tsx,jsx}\"",
    "lint": "eslint . --ignore-path .gitignore",
    "prepare": "husky install",
    "prettier": "prettier . --write --ignore-path .gitignore",
    "release": "standard-version",
    "release:prerelease": "standard-version --prerelease alpha",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gpsystem/gps.git"
  },
  "keywords": [],
  "contributors": [
    "Nicolas Rodriguez (https://github.com/nicolas377)"
  ],
  "author": "GeoFS Plugin System",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gpsystem/gps/issues"
  },
  "homepage": "https://github.com/gpsystem/gps#readme",
  "devDependencies": {
    "@commitlint/cli": "~16.2.3",
    "@commitlint/config-conventional": "~16.2.1",
    "@geps/geofs-types": "~1.0.1",
    "@types/fs-extra": "~9.0.13",
    "@types/glob": "~7.2.0",
    "@types/html-minifier": "~4.0.2",
    "@types/jest": "~27.4.1",
    "@types/react": "~18.0.5",
    "@types/react-dom": "~18.0.1",
    "@types/tailwindcss": "~3.0.10",
    "@types/yargs": "~17.0.10",
    "@typescript-eslint/eslint-plugin": "~5.19.0",
    "autoprefixer": "~10.4.4",
    "babel-loader": "~8.2.4",
    "cheerio": "~1.0.0-rc.10",
    "chokidar": "~3.5.3",
    "cspell": "~5.19.7",
    "cssnano": "~5.1.7",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-react": "~7.29.4",
    "fs-extra": "~10.1.0",
    "glob": "~7.2.0",
    "html-minifier": "~4.0.0",
    "husky": "~7.0.4",
    "jest": "~27.5.1",
    "jest-chrome": "~0.7.2",
    "postcss": "~8.4.12",
    "prettier": "~2.6.2",
    "react": "~18.0.0",
    "react-dom": "~18.0.0",
    "standard-version": "~9.3.2",
    "tailwindcss": "~3.0.23",
    "ts-jest": "~27.1.4",
    "ts-loader": "~9.2.8",
    "ts-node": "~10.7.0",
    "typescript": "~4.6.3",
    "webpack": "~5.72.0",
    "yargs": "~17.4.0"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "npm run prettier"
    },
    "bumpFiles": [
      {
        "filename": "src/manifest.json",
        "type": "json"
      },
      {
        "filename": "package.json",
        "type": "json"
      }
    ]
  }
}
